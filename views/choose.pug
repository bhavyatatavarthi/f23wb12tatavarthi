extends layout
 
block content
  h1= title
  p Welcome to #{title}
 
  - const image_names = ["bird.jpg", "wren.jpg", "birds.jpg", "hawk.jpg", "Hum.jpg"]
  - for (let i = 0; i < 3; i++)
    - const randomIndex = Math.floor(Math.random() * 5)
    img(
      src=`/images/${image_names[randomIndex]}`,
      alt=image_names[randomIndex],
      width="300",
      height="300"
    )
  